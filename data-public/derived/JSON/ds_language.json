    "last_updated": "2025-09-12",
    "fields": {
      "year": "Calendar year",
      "language": "Language of publication (English names)",
      "language_ua": "Language name in Ukrainian",
      "title_count": "Number of unique book titles published in this language",
      "copy_count": "Total number of book copies published in this language (in thousands)"
    },
    "key_languages": {
      "primary": ["Ukrainian", "Russian"],
      "emerging": ["English", "German", "Polish", "Hungarian", "Romanian"],
      "minority": ["Crimean Tatar", "Gagauz", "Rusyn", "Multiple languages (various world languages)"]
    }
  },
  "summary_statistics": {
    "total_years": 20,
    "year_range": "2005-2024",
    "total_languages_tracked": 34,
