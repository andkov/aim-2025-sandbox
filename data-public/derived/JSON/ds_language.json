    },
    "language_diversity_trends": {
      "2005-2013": "Only Ukrainian and Russian publications recorded",
      "2014": "Beginning of language diversification",
      "2018-2024": "Significant increase in multilingual publishing, emergence of minority language publications"
    }
  },
  "historical_context": {
    "key_periods": [
      {
        "period": "2005-2013",
        "description": "Pre-conflict era",
        "characteristics": ["Two-language system (Ukrainian/Russian)", "Steady growth in both languages", "Russian publications peak in 2013"]
